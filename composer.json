{
    "name": "kisscool-fr/ghibliql",
    "description": "GraphQL API wrapper to Ghibli REST API",
    "type": "project",
    "license": "proprietary",
    "homepage": "https://ghibliql.herokuapp.com",
    "authors": [
        {
            "name": "KisSCoOl",
            "email": "kisscool@kisscool.net",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "GhibliQL\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GhibliQL\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.1.7"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^8.1",
        "doctrine/cache": "^1.12",
        "guzzlehttp/guzzle": "^7.4",
        "predis/predis": "^2.0",
        "webonyx/graphql-php": "^14.11"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpstan/phpstan": "^1.5",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master"
    }
}
