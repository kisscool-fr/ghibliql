{
    "name": "kisscool-fr/ghibliql",
    "description": "GraphQL API wrapper to Ghibli REST API",
    "type": "project",
    "license": "proprietary",
    "homepage": "https://ghibliql.herokuapp.com",
    "authors": [
        {
            "name": "KisSCoOl",
            "email": "kisscool@kisscool.net",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "GhibliQL\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GhibliQL\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4.16",
            "ext-sqreen": "1.23.0"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "ext-sqreen": "*",
        "doctrine/cache": "^1.10",
        "guzzlehttp/guzzle": "^7.3",
        "predis/predis": "^1.1",
        "webonyx/graphql-php": "^14.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master"
    }
}
