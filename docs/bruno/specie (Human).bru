meta {
  name: specie (Human)
  type: graphql
  seq: 6
}

post {
  url: {{baseURL}}
  body: graphql
  auth: none
}

headers {
  Content-Type: application/json
}

body:graphql {
  query ($specieId: ID!) {
    specie(id: $specieId) {
      name
      classification
      eye_colors
      hair_colors
      people {
        name
      }
      films {
        title
      }
    }
  }
  
}

body:graphql:vars {
  {
    "specieId": "af3910a6-429f-4c74-9ad5-dfe1c4aa04f2"
  }
}
